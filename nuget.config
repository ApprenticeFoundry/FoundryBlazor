<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="FoundryBlazor" value="https://pkgs.dev.azure.com/iobt/IoBTNuGet/_packaging/FoundryBlazor/nuget/v3/index.json" />
    <add key="IoBTMessageLibrary" value="https://pkgs.dev.azure.com/iobt/IoBTNuGet/_packaging/IoBTMessageLibrary/nuget/v3/index.json" />
    <add key="BlazorThreeJS" value="https://pkgs.dev.azure.com/iobt/IoBTNuGet/_packaging/BlazorThreeJS/nuget/v3/index.json" />
    <add key="BlazorExtensionsCanvas" value="https://pkgs.dev.azure.com/iobt/IoBTNuGet/_packaging/BlazorExtensionsCanvas/nuget/v3/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <FoundryBlazor>
      <add key="Username" value="%NugetUserId%" />
      <add key="ClearTextPassword" value="%NugetPersonalAccessToken%" />
    </FoundryBlazor>
    <IoBTMessageLibrary>
      <add key="Username" value="%NugetUserId%" />
      <add key="ClearTextPassword" value="%NugetPersonalAccessToken%" />
    </IoBTMessageLibrary>
    <BlazorThreeJS>
      <add key="Username" value="%NugetUserId%" />
      <add key="ClearTextPassword" value="%NugetPersonalAccessToken%" />
    </BlazorThreeJS>
    <BlazorExtensionsCanvas>
      <add key="Username" value="%NugetUserId%" />
      <add key="ClearTextPassword" value="%NugetPersonalAccessToken%" />
    </BlazorExtensionsCanvas>
  </packageSourceCredentials>
</configuration>
